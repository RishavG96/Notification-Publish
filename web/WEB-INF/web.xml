<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <context-param>
        <param-name>hello</param-name>
        <param-value>good</param-value>
    </context-param>
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>com.prowessapps.MyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddNotice</servlet-name>
        <servlet-class>com.prowessapps.AddNotice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StoringNotice</servlet-name>
        <servlet-class>com.prowessapps.StoringNotice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>com.prowessapps.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/MyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddNotice</servlet-name>
        <url-pattern>/AddNotice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StoringNotice</servlet-name>
        <url-pattern>/StoringNotice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
